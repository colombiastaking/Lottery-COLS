# from your repo clone
git checkout -b fix/ignore-subdir-cargo-locks

# add the ignore
mkdir -p meta
echo "Cargo.lock" > meta/.gitignore
git add meta/.gitignore

# if any Cargo.lock files are already tracked, untrack them
git rm --cached meta/Cargo.lock || true
git rm --cached wasm/Cargo.lock || true

git commit -m "Ignore autogenerated Cargo.lock files in meta/ and wasm/ to fix Spawnable builds"
git push -u origin fix/ignore-subdir-cargo-locks
# then open a PR on GitHub and merge
